            "version_group": {
                "name": "ultra-sun-ultra-moon",
                "url": "/api/v2/version-group/18/"
            }
        },
        {
            "flavor_text": "The Pok\u00e9mon is protected by its white smoke, which\nprevents other Pok\u00e9mon from lowering its stats.",
            "language": {
