                {
                    "effect": "Paralysis cannot prevent the Pok\u00e9mon from moving, though the Speed cut is unaffected.  Poison still damages the Pok\u00e9mon outside of battle.",
                    "language": {
                        "name": "en",
                        "url": "/api/v2/language/9/"
                    }
                }
            ],
            "version_group": {
