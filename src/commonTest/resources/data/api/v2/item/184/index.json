            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u30dd\u30b1\u30e2\u30f3\u306b\u3000\u6301\u305f\u305b\u308b\u3068\n\u30d4\u30f3\u30c1\u306e\u3068\u304d\u3000\u3069\u308c\u304b\u3000\uff11\u3064\u306e\n\u80fd\u529b\u304c\u3000\u3050\u30fc\u3093\u3068\u3000\u3042\u304c\u308b\u3002",
