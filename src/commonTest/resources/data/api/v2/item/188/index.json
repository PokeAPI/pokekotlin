            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u30dd\u30b1\u30e2\u30f3\u306b\u3000\u6301\u305f\u305b\u308b\u3068\n\u7269\u7406\u6280\u3092\u3000\u53d7\u3051\u305f\u3068\u304d\n\u76f8\u624b\u306b\u3082\u3000\u30c0\u30e1\u30fc\u30b8\u3092\u3000\u3042\u305f\u3048\u308b\u3002",
