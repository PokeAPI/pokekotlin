        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u3061\u3087\u3063\u3068\u3000\u3053\u3060\u308f\u3063\u305f\u3000\u30e1\u30ac\u30cd\u3002\n\u6301\u305f\u305b\u308b\u3068\u3000\u7279\u653b\u306f\u3000\u3042\u304c\u308b\u304c\n\u540c\u3058\u3000\u6280\u3057\u304b\u3000\u3060\u305b\u306a\u304f\u306a\u308b\u3002",
