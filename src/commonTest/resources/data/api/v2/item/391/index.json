            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u76f8\u624b\u3092\u3000\u6012\u3089\u305b\u3066\u3000\u6df7\u4e71\u3055\u305b\u308b\u3002\n\u6012\u308a\u3067\u3000\u76f8\u624b\u306e\u3000\u653b\u6483\u306f\n\u3050\u30fc\u3093\u3068\u3000\u3042\u304c\u3063\u3066\u3057\u307e\u3046\u3002",
