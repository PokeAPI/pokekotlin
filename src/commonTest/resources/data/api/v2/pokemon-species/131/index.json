        {
            "flavor_text": "\u3072\u3068\u3092\u3000\u306e\u305b\u3066\u3000\u304a\u3088\u3050\u306e\u304c\u3000\u3059\u304d\u3002\n\u30a2\u30ed\u30fc\u30e9\u3061\u307b\u3046\u3000\u3067\u306f\u3000\u305f\u3044\u305b\u3064\u306a\n\u3059\u3044\u3058\u3087\u3046\u306e\u3000\u3053\u3046\u3064\u3046\u3000\u3057\u3085\u3060\u3093\u3002",
