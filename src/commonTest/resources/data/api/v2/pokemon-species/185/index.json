        },
        {
            "flavor_text": "\u304a\u5e74\u5bc4\u308a\u306b\u3000\u4eba\u6c17\u3067\u3000\u5c02\u9580\u8a8c\u3082\n\u3042\u308b\u307b\u3069\u3002\u3000\u611b\u597d\u5bb6\u306f\u3000\u8155\u306e\n\u9577\u3055\u3084\u3000\u89d2\u5ea6\u306b\u3000\u3053\u3060\u308f\u308b\u306e\u3060\u3002",
