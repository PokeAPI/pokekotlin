        {
            "flavor_text": "\u3042\u305f\u307e\u306e\u3000\u306a\u304b\u3067\u306f\u3000\u3064\u306d\u306b\u3000\u305b\u304b\u3044\u3092\n\u304b\u3048\u308b\u3088\u3046\u306a\u3000\u30a2\u30a4\u30c7\u30a2\u304c\u3000\u308f\u304d\u3060\u3057\u3066\n\u3044\u308b\u304c\u3000\u3072\u3089\u3081\u304f\u306f\u3058\u304b\u3089\u3000\u308f\u3059\u308c\u308b\u3002",
