            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\uba38\ub9bf\uc18d\uc73c\ub85c\ub294 \ud56d\uc0c1 \uc138\uacc4\ub97c \ubc14\uafc0\n\uc815\ub3c4\uc758 \uc544\uc774\ub514\uc5b4\uac00 \ub118\uccd0 \ud750\ub974\uc9c0\ub9cc\n\ubc88\ub729\uc774\ub294 \uc21c\uac04 \uc78a\uc5b4\ubc84\ub9b0\ub2e4.",
