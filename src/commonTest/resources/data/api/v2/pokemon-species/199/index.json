        },
        {
            "flavor_text": "\u307e\u3088\u3063\u305f\u3089\u3000\u30e4\u30c9\u30ad\u30f3\u30b0\u306b\u304d\u3051\u3000\u3068\u3044\u3046\n\u3053\u3068\u308f\u3056\u304c\u3000\u306e\u3053\u308b\u3000\u3061\u3044\u304d\u3082\u3000\u3042\u308b\u307b\u3069\n\u304b\u3057\u3053\u3044\u3053\u3068\u3067\u3000\u3086\u3046\u3081\u3044\u3000\u306a\u306e\u3060\u3002",
