    "flavor_text_entries": [
        {
            "flavor_text": "\u636e\u8bf4\u4f1a\u7528\u540e\u811a\u4e0a\u5c0f\u5c0f\u7684\u8e7c\n\u6cc5\u6c34\u6e21\u6d77\uff0c\u7a7f\u8d8a\u5c9b\u5c7f\n\u6765\u8eb2\u907f\u654c\u4eba\u7684\u8ffd\u51fb\u3002",
