            }
        },
        {
            "flavor_text": "\u707d\u5bb3\u306e\u3000\u4e88\u5146\u3092\u3000\u611f\u3058\u308b\u3068\n\u59ff\u3092\u3000\u898b\u305b\u308b\u3000\u305f\u3081\u306b\u3000\u707d\u3044\u3092\u3000\u547c\u3076\n\u30dd\u30b1\u30e2\u30f3\u3068\u3000\u8aa4\u89e3\u3055\u308c\u3066\u3044\u305f\u3002",
