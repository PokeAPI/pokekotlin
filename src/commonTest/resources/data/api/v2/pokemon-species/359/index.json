        {
            "flavor_text": "\u4e0d\u5409\u306a\u306e\u306f\u3000\u898b\u305f\u76ee\u3060\u3051\u3002\u3000\u7530\u7551\u3092\n\u5b88\u3063\u3066\u304f\u308c\u305f\u308a\u3000\u7570\u5909\u3092\u3000\u4eba\u306b\n\u544a\u3052\u3066\u304f\u308c\u308b\u3000\u3042\u308a\u304c\u305f\u3044\u3000\u5b58\u5728\u3002",
