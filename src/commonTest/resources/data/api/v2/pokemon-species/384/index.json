        {
            "flavor_text": "\u30aa\u30be\u30f3\u5c64\u3092\u3000\u98db\u3073\u7d9a\u3051\u3000\u30a8\u30b5\u3068\u306a\u308b\n\u9695\u77f3\u3092\u3000\u304f\u3089\u3046\u3002\u4f53\u5185\u306b\u3000\u305f\u307e\u3063\u305f\n\u9695\u77f3\u306e\u3000\u30a8\u30cd\u30eb\u30ae\u30fc\u3067\u3000\u30e1\u30ac\u30b7\u30f3\u30ab\u3059\u308b\u3002",
