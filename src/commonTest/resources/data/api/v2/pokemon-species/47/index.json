    "flavor_text_entries": [
        {
            "flavor_text": "\u5e95\u4e0b\u7684\u866b\u5b50\u57fa\u672c\u4e0a\u6b7b\u4e86\uff0c\n\u771f\u6b63\u7684\u672c\u4f53\u662f\u80cc\u4e0a\u7684\u8611\u83c7\u3002\n\u5982\u679c\u8611\u83c7\u6389\u4e0b\u6765\uff0c\u5b83\u5c31\u4e0d\u4f1a\u52a8\u4e86\u3002",
