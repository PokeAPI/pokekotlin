            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
            }
        },
        {
            "flavor_text": "Capta con l\u2019antenna onde che arrivano dal mondo degli spiriti,\nche gli ordinano di portare l\u00ec le persone.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
            }
        },
        {
