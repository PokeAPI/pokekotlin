    "flavor_text_entries": [
        {
            "flavor_text": "\u5c3b\u5c3e\u3067\u3000\u592a\u967d\u306e\u5149\u3092\u3000\u6d74\u3073\u3066\n\u5149\u5408\u6210\u3092\u3000\u3059\u308b\u3002\u5143\u6c17\u3092\u3000\u306a\u304f\u3059\u3068\n\u5c3b\u5c3e\u304c\u3000\u305f\u308c\u3055\u304c\u308b\u3002",
