    "flavor_text_entries": [
        {
            "flavor_text": "\u8b66\u6212\u5fc3\u304c\u3000\u5f37\u304f\u3000\u304b\u306a\u3089\u305a\n\uff11\u5339\u306f\u3000\u898b\u5f35\u308a\u3092\u3000\u3057\u3066\u3044\u308b\u306e\u3060\u304c\n\u5f8c\u308d\u304b\u3089\u306e\u3000\u6575\u306b\u306f\u3000\u6c17\u3065\u304b\u306a\u3044\u3002",
