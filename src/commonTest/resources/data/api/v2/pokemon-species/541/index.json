    "flavor_text_entries": [
        {
            "flavor_text": "\u8449\u3063\u3071\u3067\u3000\u4f53\u3092\u3000\u5305\u202f\u3053\u3093\u3067\n\u5bd2\u3055\u3092\u3000\u9632\u3050\u3002\u624b\u8fd1\u306a\u3000\u843d\u3061\u8449\u3092\n\u98df\u3079\u306a\u304c\u3089\u3000\u68ee\u3092\u3000\u79fb\u52d5\u3059\u308b\u3002",
