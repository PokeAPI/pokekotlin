        },
        {
            "flavor_text": "\u786c\u3044\u3000\u6bbb\u306b\u3000\u5b88\u3089\u308c\u3066\u3044\u308b\u3002\n\u30bf\u30a4\u30e4\u306e\u3088\u3046\u306b\u3000\u56de\u8ee2\u3057\u3066\n\u6575\u306b\u3000\u6fc0\u3057\u304f\u3000\u4f53\u5f53\u305f\u308a\u3059\u308b\u3002",
