    "flavor_text_entries": [
        {
            "flavor_text": "\u666e\u6bb5\u306f\u3000\u52d5\u304b\u306a\u3044\u304c\n\u8972\u308f\u308c\u308b\u3068\u3000\u9ad8\u901f\u3000\u56de\u8ee2\u3057\u3066\u3000\u8d70\u308a\u56de\u308a\n\u4f53\u5f53\u305f\u308a\u3067\u3000\u53cd\u6483\u3059\u308b\u306e\u3060\u3002",
