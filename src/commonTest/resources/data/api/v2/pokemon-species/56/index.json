        },
        {
            "flavor_text": "\u3061\u3087\u3063\u3068\u3057\u305f\u3053\u3068\u3067\u3000\u3059\u3050\u3000\u304a\u3053\u308b\u3002\n\u30b9\u30c8\u30ec\u30b9\u3092\u3000\u305f\u3081\u3053\u307e\u306a\u3044\u306e\u3067\n\u304b\u306a\u308a\u3000\u306a\u304c\u3044\u304d\u3059\u308b\u3000\u30dd\u30b1\u30e2\u30f3\u3060\u3002",
