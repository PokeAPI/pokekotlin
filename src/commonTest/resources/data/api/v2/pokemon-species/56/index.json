    "flavor_text_entries": [
        {
            "flavor_text": "\u8be5\u5b9d\u53ef\u68a6\u4f1a\u56e0\u4e00\u70b9\u5c0f\u4e8b\u800c\u7acb\u523b\u52a8\u6012\u3002\n\u7531\u4e8e\u4e0d\u4f1a\u79ef\u7d2f\u538b\u529b\uff0c\n\u6240\u4ee5\u80fd\u6d3b\u76f8\u5f53\u957f\u7684\u65f6\u95f4\u3002",
