        },
        {
            "flavor_text": "\u3044\u304b\u308a\u3061\u3089\u3057\u3066\u3000\u3064\u304b\u308c\u3066\u3000\u306d\u3080\u308b\u304c\n\u3086\u3081\u306e\u3000\u306a\u304b\u3067\u3000\u306f\u3089\u3092\u305f\u3066\u3000\u3044\u304b\u308a\u3067\n\u3081\u3056\u3081\u3000\u3081\u3056\u3081\u305f\u3053\u3068\u306b\u3000\u307e\u305f\u3044\u304b\u308b\u3002",
