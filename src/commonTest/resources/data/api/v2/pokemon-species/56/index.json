        {
            "flavor_text": "\u4f53\u304c\u3000\u9707\u3048\u3000\u9f3b\u606f\u304c\u3000\u8352\u304f\u306a\u308c\u3070\n\u6012\u308a\u3060\u3059\u3000\u524d\u89e6\u308c\u3000\u306a\u306e\u3060\u304c\u3000\u3042\u3063\u3068\u3044\u3046\u9593\u306b\n\u6fc0\u3057\u304f\u3000\u6012\u308b\u306e\u3067\u3000\u9003\u3052\u51fa\u3059\u3000\u6687\u306f\u3000\u306a\u3044\u3002",
