    "flavor_text_entries": [
        {
            "flavor_text": "\u4f53\u304c\u3000\u4e7e\u304f\u3068\u3000\u5f31\u3063\u3066\u3057\u307e\u3046\u3002\n\u8584\u3044\u3000\u819c\u3092\u3000\u4f55\u91cd\u3082\u3000\u5dfb\u3044\u3066\n\u4e7e\u71e5\u3092\u3000\u9632\u3044\u3067\u3044\u308b\u306e\u3060\u3002",
