        },
        {
            "flavor_text": "Si riscalda assorbendo la luce del sole dalle ali. Quando la sua\ntemperatura si abbassa, non riesce pi\u00f9 a muoversi.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
