    "flavor_text_entries": [
        {
            "flavor_text": "\u6b63\u9762\u4f24\u8d8a\u591a\uff0c\u5c31\u8868\u793a\u8d8a\u52c7\u6562\uff0c\n\u800c\u80cc\u9762\u53d7\u4f24\u591a\u7684\u5219\u4f1a\u5728\u7fa4\u4f53\u4e2d\n\u53d7\u5230\u8f7b\u89c6\u3002",
