    "flavor_text_entries": [
        {
            "flavor_text": "\u4ece\uff15\u53ea\u89d2\u4e2d\u55b7\u51fa\u706b\u7130\u4e0e\u5bf9\u624b\u6218\u6597\u3002\n\u90a3\u6e29\u5ea6\u6700\u9ad8\u53ef\u8fbe\uff13\uff10\uff10\uff10\u5ea6\u3002",
