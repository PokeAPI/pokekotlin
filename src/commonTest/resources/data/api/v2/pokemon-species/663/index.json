    "flavor_text_entries": [
        {
            "flavor_text": "\u7fbd\u6bdb\u5f88\u7ed3\u5b9e\uff0c\u8fde\u706b\u90fd\u70e7\u4e0d\u7a7f\u3002\n\u8fc7\u53bb\u7684\u6d88\u9632\u5458\u670d\u5c31\u662f\u7528\n\u70c8\u7bad\u9e70\u7684\u7fbd\u6bdb\u505a\u6210\u7684\u3002",
