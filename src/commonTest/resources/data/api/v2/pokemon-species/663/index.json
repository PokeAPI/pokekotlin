            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\ub0a0\uac1c\ub294 \ubd88\uc774 \ubd99\uc9c0 \uc54a\uace0 \ud2bc\ud2bc\ud558\ub2e4.\n\uc61b\ub0a0\uc5d0\ub294 \uc18c\ubc29\uad00\ub4e4\uc758 \uc637\uc744\n\ud30c\uc774\uc5b4\ub85c\uc758 \ub0a0\uac1c\ub85c \ub9cc\ub4e4\uc5c8\ub2e4.",
