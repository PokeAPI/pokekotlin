            "version": {
                "name": "moon",
                "url": "/api/v2/version/28/"
            }
        },
        {
            "flavor_text": "It zooms directly at its prey at flight speeds of\nclose to 310 mph, while fiery embers scatter\nfrom gaps in its feathers.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
