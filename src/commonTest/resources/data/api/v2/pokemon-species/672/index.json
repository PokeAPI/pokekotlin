    "flavor_text_entries": [
        {
            "flavor_text": "\u6c34\u3068\u3000\u592a\u967d\u304c\u3000\u3042\u308c\u3070\u3000\u80cc\u4e2d\u306e\n\u8449\u3063\u3071\u3067\u3000\u30a8\u30cd\u30eb\u30ae\u30fc\u304c\u3000\u4f5c\u308c\u308b\u306e\u3067\n\u30a8\u30b5\u3092\u3000\u98df\u3079\u306a\u304f\u3066\u3082\u3000\u5e73\u6c17\u306a\u306e\u3060\u3002",
