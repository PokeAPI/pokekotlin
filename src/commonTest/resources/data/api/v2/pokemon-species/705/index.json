        {
            "flavor_text": "\u6b6f\u304c\u3000\u306a\u3044\u306e\u3067\u3000\u306a\u3093\u3067\u3082\u3000\u6eb6\u304b\u3059\n\u7c98\u6db2\u3092\u3000\u7372\u7269\u306b\u3000\u304b\u3051\u3066\n\u6eb6\u304b\u3057\u3066\u304b\u3089\u3000\u3059\u3059\u3063\u3066\u3000\u55b0\u3089\u3046\u3002",
