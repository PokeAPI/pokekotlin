    "flavor_text_entries": [
        {
            "flavor_text": "\u6027\u683c\u66b4\u8e81\u53c8\u4efb\u6027\uff0c\n\u4f46\u8ba8\u538c\u6b3a\u51cc\u5f31\u5c0f\u8fd9\u7c7b\u65e0\u804a\u7684\u4e8b\u3002\n\u5bf9\u4e8e\u5f3a\u5927\u7684\u5bf9\u624b\u4f1a\u62ff\u51fa\u5e72\u52b2\u6765\u3002",
