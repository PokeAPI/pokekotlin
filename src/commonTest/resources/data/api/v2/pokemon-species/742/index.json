        },
        {
            "flavor_text": "\u82b1\u306b\u3000\u4f3c\u305f\u3000\u30aa\u30fc\u30e9\u3092\u3000\u6301\u3064\u3000\u4eba\u306e\n\u982d\u306e\u3000\u4e0a\u306b\u306f\u3000\u305f\u304f\u3055\u3093\u306e\n\u30a2\u30d6\u30ea\u30fc\u304c\u3000\u96c6\u307e\u3063\u3066\u304f\u308b\u3088\u3002",
