            "version": {
                "name": "moon",
                "url": "/api/v2/version/28/"
            }
        },
        {
            "flavor_text": "\u64c1\u6709\u50cf\u82b1\u4e00\u822c\u6c23\u5834\u7684\u4eba\uff0c\n\u982d\u4e0a\u6703\u6709\u8a31\u591a\u840c\u867b\u805a\u96c6\u904e\u4f86\u54e6\u3002",
