    "flavor_text_entries": [
        {
            "flavor_text": "\u559c\u6b22\u5403\u82b1\u871c\u548c\u82b1\u7c89\u3002\n\u4f1a\u5728\u82b1\u5703\u91cc\u5bfb\u627e\u98df\u7269\uff0c\n\u548c\u5df4\u5927\u8776\u5c55\u5f00\u5c0f\u51b2\u7a81\u3002",
