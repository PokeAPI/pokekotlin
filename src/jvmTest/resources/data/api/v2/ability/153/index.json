    "effect_entries": [
        {
            "effect": "This Pok\u00e9mon's Attack rises one stage upon knocking out another Pok\u00e9mon, even a friendly Pok\u00e9mon.\n\nThis ability does not take effect when the Pok\u00e9mon indirectly causes another Pok\u00e9mon to faint, e.g. through poison or spikes.\n\nIf this Pok\u00e9mon knocks out a Pok\u00e9mon with mummy, the former's ability will change without taking effect.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "short_effect": "Raises Attack one stage upon KOing a Pok\u00e9mon."
        }
    ],
