    "flavor_text_entries": [
        {
            "flavor_text": "\u6253\u5012\u5bf9\u624b\u7684\u65f6\u5019\uff0c\n\u81ea\u5df1\u6700\u9ad8\u7684\u90a3\u9879\u80fd\u529b\u4f1a\u63d0\u9ad8\u3002",
