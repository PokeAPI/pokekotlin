            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u92ed\u304f\u3000\u3068\u304c\u3063\u305f\u3000\u30ad\u30d0\u3002\n\u6301\u305f\u305b\u308b\u3068\u3000\u30c0\u30e1\u30fc\u30b8\u3092\u3000\u4e0e\u3048\u305f\u3068\u304d\n\u3072\u308b\u307e\u305b\u308b\u3053\u3068\u304c\u3000\u3042\u308b\u3002",
