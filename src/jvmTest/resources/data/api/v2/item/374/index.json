            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\uff15\u30bf\u30fc\u30f3\u306e\u3000\u9593\u3000\u7269\u7406\u3068\n\u7279\u6b8a\u306e\u3000\u30c0\u30e1\u30fc\u30b8\u3092\u3000\u5f31\u3081\u308b\u3002\n\u3042\u3089\u308c\u306e\u3000\u6642\u3057\u304b\u3000\u51fa\u305b\u306a\u3044\u3002",
