            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u81ea\u5206\u306b\u3000\u6697\u793a\u3092\u3000\u304b\u3051\u308b\u3053\u3068\u3067\n\u80fd\u529b\u5909\u5316\u306e\u3000\u72b6\u614b\u3092\n\u76f8\u624b\u3068\u3000\u540c\u3058\u306b\u3059\u308b\u3002",
