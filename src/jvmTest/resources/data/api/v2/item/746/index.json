        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u4f53\u304b\u3089\u3000\u60aa\u610f\u306b\u3000\u307f\u3061\u305f\n\u6050\u308d\u3057\u3044\u3000\u30aa\u30fc\u30e9\u3092\u3000\u767a\u3059\u308b\u3002\n\u76f8\u624b\u3092\u3000\u3072\u308b\u307e\u305b\u308b\u3053\u3068\u304c\u3000\u3042\u308b\u3002",
