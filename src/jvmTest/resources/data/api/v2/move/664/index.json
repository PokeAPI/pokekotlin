    "flavor_text_entries": [
        {
            "flavor_text": "\u968f\u7740\u5531\u6b4c\u4f1a\u653e\u51fa\u5f88\u591a\u6c14\u7403\u3002\n\u53d7\u5230\u6b64\u62db\u5f0f\u653b\u51fb\u65f6\uff0c\n\u707c\u4f24\u4f1a\u88ab\u6cbb\u6108\u3002",
