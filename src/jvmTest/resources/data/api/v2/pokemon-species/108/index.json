        },
        {
            "flavor_text": "\u306a\u304c\u3044\u3000\u30d9\u30ed\u3067\u3000\u306a\u3093\u3067\u3082\u3000\u3079\u308d\u308a\u3068\n\u306a\u3081\u3066\u3000\u305f\u3057\u304b\u3081\u3066\u3044\u308b\u3002\u3000\u306a\u3081\u3089\u308c\u305f\n\u3076\u3076\u3093\u3092\u3000\u307b\u3046\u3063\u3066\u304a\u304f\u3068\u3000\u304b\u3076\u308c\u308b\u305e\u3002",
