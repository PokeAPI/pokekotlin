    "flavor_text_entries": [
        {
            "flavor_text": "\u4f1a\u7ec4\u6210\u4e00\u4e2a\u7fa4\u4f53\u751f\u6d3b\u3002\n\u5728\u7fa4\u4f53\u4e2d\uff0c\u8c01\u7684\u89d2\u6700\u7c97\u6700\u957f\uff0c\n\u5e76\u4f24\u75d5\u7d2f\u7d2f\uff0c\u8c01\u5c31\u662f\u8001\u5927\u3002",
