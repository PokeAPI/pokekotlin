        {
            "flavor_text": "\u591c\u3054\u3068\u3000\u7372\u7269\u3092\u3000\u6c42\u3081\u3066\u3000\u3055\u8ff7\u3046\u3002\n\u898b\u3064\u3051\u305f\u3000\u7372\u7269\u306b\u3000\u7cf8\u3092\u3000\u5410\u304d\n\u8eab\u52d5\u304d\u3092\u3000\u5c01\u3058\u3066\u3000\u30ad\u30d0\u3067\u3000\u304b\u3058\u308b\u3002",
