            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "Nonostante emetta un verso chiassoso,\nun compositore si \u00e8 ispirato alle sue grida\nper scrivere delle splendide ballate.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
