            "version": {
                "name": "moon",
                "url": "/api/v2/version/28/"
            }
        },
        {
            "flavor_text": "\u5982\u679c\u5f9e\u56db\u4e0b\u7121\u4eba\u7684\u623f\u9593\u88e1\n\u50b3\u4f86\u62bd\u6ce3\u822c\u7684\u8072\u97f3\uff0c\n\u90a3\u4e00\u5b9a\u662f\u5922\u5996\u7684\u60e1\u4f5c\u5287\u3002",
