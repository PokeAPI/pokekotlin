        },
        {
            "flavor_text": "\u81ea\u5206\u306e\u30c6\u30ea\u30c8\u30ea\u30fc\u3092\u3000\u5b88\u308b\u305f\u3081\u306a\u3089\n\u5927\u304d\u306a\u3000\u30dd\u30b1\u30e2\u30f3\u304c\u3000\u76f8\u624b\u3067\u3082\n\u5411\u304b\u3063\u3066\u3044\u304f\u3000\u5411\u3053\u3046\u898b\u305a\u306a\u3000\u6027\u8cea\u3002",
