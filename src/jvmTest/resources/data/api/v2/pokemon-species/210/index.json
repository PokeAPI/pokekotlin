    "flavor_text_entries": [
        {
            "flavor_text": "\u989a\u7684\u529b\u91cf\u5f88\u5f3a\uff0c\u4f46\u4e0d\u559c\u6b22\u4e89\u6597\uff0c\n\u6240\u4ee5\u5f88\u5c11\u6709\u5c55\u9732\u7684\u673a\u4f1a\u3002",
