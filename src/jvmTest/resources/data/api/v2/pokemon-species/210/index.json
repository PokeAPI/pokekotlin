            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u30a2\u30b4\u306e\u3000\u3061\u304b\u3089\u306f\u3000\u304d\u3087\u3046\u308a\u3087\u304f\u3060\u304c\n\u3042\u3089\u305d\u3044\u3092\u3000\u3053\u306e\u307e\u306a\u3044\u306e\u3067\u3000\u3081\u3063\u305f\u306b\n\u3072\u308d\u3046\u3059\u308b\u3000\u304d\u304b\u3044\u306f\u3000\u306a\u3044\u306e\u3060\u3002",
