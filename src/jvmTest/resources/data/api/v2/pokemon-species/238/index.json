        {
            "flavor_text": "\u306a\u306b\u304b\u3092\u3000\u78ba\u304b\u3081\u308b\u3068\u304d\u306f\u3000\u624b\u3000\u3088\u308a\n\u5148\u306b\u3000\u5507\u3067\u3000\u89e6\u308c\u3066\u307f\u308b\u3002\n\u3053\u307e\u3081\u306b\u3000\u8210\u3081\u3066\u3000\u6c5a\u308c\u3092\u3000\u843d\u3068\u3059\u3002",
