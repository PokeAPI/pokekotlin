        {
            "flavor_text": "\u306f\u306a\u304b\u3089\u3000\u3061\u3087\u308d\u3061\u3087\u308d\u3068\u3000\u307b\u306e\u304a\u304c\n\u3082\u308c\u3066\u3044\u308b\u3068\u304d\u306f\u3000\u304b\u305c\u3092\u3000\u3072\u3044\u3066\u3044\u308b\u3002\n\u30de\u30b0\u30de\u306e\u306a\u304b\u3067\u3000\u3084\u3059\u307e\u305b\u3066\u3000\u3042\u3052\u3066\u306d\u3002",
