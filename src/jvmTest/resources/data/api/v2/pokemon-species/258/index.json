        {
            "flavor_text": "\u6c34\u4e2d\u3067\u306f\u3000\u307b\u3063\u307a\u306e\u3000\u30a8\u30e9\u3067\u3000\u547c\u5438\u3059\u308b\u3002\n\u30d4\u30f3\u30c1\u306b\u3000\u306a\u308b\u3068\u3000\u4f53\u3088\u308a\u3000\u5927\u304d\u306a\u3000\u5ca9\u3092\n\u7c89\u3005\u306b\u3000\u7815\u304f\u3000\u30d1\u30ef\u30fc\u3092\u3000\u767a\u63ee\u3059\u308b\u3002",
