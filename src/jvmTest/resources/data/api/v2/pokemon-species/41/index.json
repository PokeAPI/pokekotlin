        {
            "flavor_text": "\u592a\u967d\u306e\u3000\u5149\u3092\u3000\u6d74\u3073\u308b\u3068\u3000\u4f53\u8abf\u304c\n\u60aa\u304f\u306a\u308b\u306e\u3067\u3000\u663c\u9593\u306f\u3000\u6d1e\u7a9f\u3084\u3000\u53e4\u3073\u305f\n\u5bb6\u306e\u3000\u8ed2\u4e0b\u306b\u3000\u3076\u3089\u4e0b\u304c\u3063\u3066\u3000\u5bdd\u3066\u3044\u308b\u3002",
