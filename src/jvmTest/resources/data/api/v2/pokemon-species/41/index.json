        },
        {
            "flavor_text": "\u65e5\u306e\u3000\u5149\u3092\u3000\u6d74\u3073\u308b\u3068\u3000\u706b\u50b7\u3059\u308b\u3002\n\u4f7f\u3046\u3000\u8d85\u97f3\u6ce2\u306e\u3000\u632f\u52d5\u306f\n\u7fa4\u308c\u306b\u3000\u3088\u3063\u3066\u3000\u5fae\u5999\u306b\u3000\u9055\u3046\u3002",
