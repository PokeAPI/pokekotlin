    "flavor_text_entries": [
        {
            "flavor_text": "\u66fe\u7ecf\u4e00\u76f4\u80cc\u7740\u58f3\u3002\n\u5728\u80cc\u7684\u91cc\u9762\u6709\u7740\n\u53c8\u786c\u53c8\u8584\u7684\u58f3\u7684\u75d5\u8ff9\u3002",
