            "language": {
                "name": "es",
                "url": "/api/v2/language/7/"
            },
            "version": {
                "name": "omega-ruby",
                "url": "/api/v2/version/25/"
            }
        },
        {
            "flavor_text": "Es lebt in H\u00f6hlen, die Erdw\u00e4rme ausgesetzt sind. Es springt\nheraus und bei\u00dft, wenn Feinde sich n\u00e4hern.",
            "language": {
