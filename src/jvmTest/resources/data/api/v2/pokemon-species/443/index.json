    "flavor_text_entries": [
        {
            "flavor_text": "\u4f1a\u5bf9\u52a8\u7684\u4e1c\u897f\u53d1\u751f\u53cd\u5e94\uff0c\n\u98de\u6251\u8fc7\u53bb\u54ac\u4f4f\u3002\u6709\u65f6\u5019\n\u4f1a\u4f24\u5230\u81ea\u5df1\uff0c\u4f46\u4e5f\u4e0d\u592a\u5728\u610f\u3002",
