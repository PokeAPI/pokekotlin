    "flavor_text_entries": [
        {
            "flavor_text": "\u5730\u4e0b\u3000\uff11\uff10\uff10\u30e1\u30fc\u30c8\u30eb\u306b\u3000\u8ff7\u8def\u306e\n\u3088\u3046\u306a\u3000\u5de3\u7a74\u3092\u3000\u4f5c\u308b\u3002\u5730\u4e0b\u9244\u306e\n\u30c8\u30f3\u30cd\u30eb\u306b\u3000\u7a74\u3092\u3000\u958b\u3051\u3066\u3057\u307e\u3046\u3002",
