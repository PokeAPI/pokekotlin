        },
        {
            "flavor_text": "\uff15\uff10\u30ad\u30ed\u3000\u5148\u306b\u3000\u3044\u308b\u3000\u7372\u7269\u3092\n\u898b\u3064\u3051\u308b\u3068\u3000\u7802\u6f20\u3092\u3000\u6cf3\u3050\u3088\u3046\u306b\n\u79fb\u52d5\u3057\u3066\u3000\u98db\u3073\u304b\u304b\u308a\u3000\u565b\u307f\u3064\u304f\u3002",
