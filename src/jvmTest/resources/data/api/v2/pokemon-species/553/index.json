            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "50km \ub5a8\uc5b4\uc838 \uc788\ub294 \uba39\uc774\ub97c\n\ubc1c\uacac\ud558\uba74 \uc0ac\ub9c9\uc744 \ud5e4\uc5c4\uce58\ub4ef\uc774\n\uc774\ub3d9\ud558\uc5ec \ub364\ubcbc\ub4e4\uc5b4 \ub365\uc11d \ubb38\ub2e4.",
