        {
            "flavor_text": "\u3061\u3073\u30ae\u30a2\u3068\u3000\u3067\u304b\u30ae\u30a2\u306b\u3000\u308f\u304b\u308c\u305f\u3002\n\u653b\u6483\u306e\u305f\u3081\u3000\u98db\u3070\u3057\u305f\u3000\u3061\u3073\u30ae\u30a2\u304c\n\u623b\u3063\u3066\u3053\u306a\u3044\u3068\u3000\u6b7b\u3093\u3067\u3057\u307e\u3046\u3002",
