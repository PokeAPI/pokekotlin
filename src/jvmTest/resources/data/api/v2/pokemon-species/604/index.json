    "flavor_text_entries": [
        {
            "flavor_text": "\u8155\u306e\u3000\u529b\u3067\u3000\u6d77\u304b\u3089\u3000\u306f\u3044\u51fa\u3057\n\u6c34\u8fba\u306e\u3000\u7372\u7269\u306b\u3000\u8972\u3044\u304b\u304b\u308b\u3002\n\u4e00\u77ac\u3067\u3000\u6d77\u3078\u3000\u5f15\u304d\u305a\u308a\u3053\u3080\u3002",
