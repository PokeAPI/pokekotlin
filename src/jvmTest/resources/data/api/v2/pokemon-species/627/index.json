        },
        {
            "flavor_text": "\u5c16\u3063\u305f\u30c4\u30e1\u3092\u3000\u7372\u7269\u306b\u3000\u7a81\u304d\u7acb\u3066\n\u3064\u3044\u3070\u3093\u3067\u3044\u308b\u3002\u3000\u6728\u306e\u5b9f\u3082\u3000\u98df\u3079\u308b\u304c\n\u57fa\u672c\u7684\u306b\u3000\u8089\u98df\u306e\u3000\u30dd\u30b1\u30e2\u30f3\u3002",
