    "flavor_text_entries": [
        {
            "flavor_text": "\u307e\u308f\u308a\u306e\u3000\u69d8\u5b50\u304c\u3000\u308f\u304b\u3089\u306a\u3044\u306e\u3067\n\u624b\u5f53\u308a\u6b21\u7b2c\u306b\u3000\u3076\u3064\u304b\u3063\u3066\u306f\n\u52d5\u304f\u3082\u306e\u306b\u3000\u304b\u202f\u3064\u304d\u3000\u98df\u3079\u307e\u304f\u308b\u3002",
