            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "Sparge scintille per incendiare i cespugli e fa\nman bassa dei Pok\u00e9mon Coleottero che saltano\nfuori spaventati.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
