    "flavor_text_entries": [
        {
            "flavor_text": "\u4f4f\u3093\u3067\u3044\u308b\u3000\u6c17\u5019\u3084\u3000\u98a8\u571f\u306b\u3088\u3063\u3066\n\u7fbd\u306e\u3000\u6a21\u69d8\u304c\u3000\u9055\u3046\u3000\u30dd\u30b1\u30e2\u30f3\u3002\n\u8272\u9bae\u3084\u304b\u306a\u3000\u308a\u3093\u3077\u3093\u3092\u3000\u307e\u304f\u3002",
