        },
        {
            "flavor_text": "\u304a\u3046\u306e\u3000\u305d\u3057\u3064\u3092\u3000\u3082\u3064\u3000\u306b\u3093\u3052\u3093\u3092\n\u202f\u306c\u304f\u3089\u3057\u3044\u3002\u202f\u3068\u3081\u3089\u308c\u305f\u3000\u3072\u3068\u306f\n\u3084\u304c\u3066\u3000\u304a\u3046\u306b\u306a\u308b\u3068\u3000\u3044\u308f\u308c\u3066\u3044\u308b\u3002",
            "language": {
                "name": "ja-Hrkt",
