    "flavor_text_entries": [
        {
            "flavor_text": "\u8179\u90e8\u62e5\u6709\u53d1\u7535\u5668\u5b98\u3002\n\u5728\u5927\u989a\u91cc\u6536\u96c6\u80fd\u91cf\uff0c\n\u91ca\u653e\u51fa\u60ca\u4eba\u7684\u7535\u51fb\u3002",
