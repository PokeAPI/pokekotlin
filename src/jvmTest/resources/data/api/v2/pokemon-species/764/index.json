            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u3042\u305f\u307e\u304b\u3089\u3000\u306d\u3070\u308a\u3051\u306e\u3000\u3042\u308b\u3000\u30c4\u30eb\u3092\n\u306e\u3070\u3057\u3066\u3000\u306f\u306a\u3092\u3000\u3064\u3093\u3067\u3000\u3058\u3076\u3093\u3092\n\u304b\u3056\u308b\u3002\u3000\u306f\u306a\u304c\u3000\u306a\u3044\u3068\u3000\u3075\u3042\u3093\u3002",
            "language": {
