            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u3075\u3060\u3093\u306f\u3000\u304d\u306e\u3046\u3048\u3067\u3000\u3081\u3044\u305d\u3046\u3057\u3066\n\u3059\u3054\u3059\u3002\u3000\u304b\u3063\u3066\u306b\u30e2\u30f3\u30b9\u30bf\u30fc\u30dc\u30fc\u30eb\u3092\n\u306a\u3052\u3066\u3000\u30dd\u30b1\u30e2\u30f3\u306b\u3000\u3081\u3044\u308c\u3044\u3059\u308b\u3002",
