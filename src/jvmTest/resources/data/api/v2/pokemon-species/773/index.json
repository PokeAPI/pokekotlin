    "flavor_text_entries": [
        {
            "flavor_text": "\u89c9\u9192\u8fdb\u5316\u540e\u7684\u6837\u5b50\u3002\n\u4ece\u6c89\u91cd\u7684\u9762\u7f69\u91cc\u88ab\u89e3\u653e\u51fa\u6765\uff0c\n\u901f\u5ea6\u5927\u5e45\u63d0\u5347\u3002",
            "language": {
