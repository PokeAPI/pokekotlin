        },
        {
            "flavor_text": "\u305d\u306e\u540d\u306f\u3000\u30bf\u30a4\u30d7\uff1a\u30cc\u30eb\u3092\u3000\u521d\u3081\u3066\n\u3053\u306e\u59ff\u306b\u3000\u9032\u5316\u3000\u3055\u305b\u305f\n\u5c11\u5e74\u306b\u3000\u3088\u3063\u3066\u3000\u540d\u3065\u3051\u3089\u308c\u305f\u3002",
