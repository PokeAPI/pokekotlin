    "flavor_text_entries": [
        {
            "flavor_text": "\u626d\u554a\u626d\u5730\u79fb\u52a8\u7740\uff0c\n\u9760\u8fd1\u4eba\u540e\u4f1a\u7a81\u7136\u8ba9\u5934\u7206\u70b8\u3002\n\u597d\u50cf\u662f\u7a76\u6781\u5f02\u517d\u4e2d\u7684\u4e00\u79cd\u3002",
