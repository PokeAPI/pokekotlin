            "version_group": {
                "name": "ultra-sun-ultra-moon",
                "url": "/api/v2/version-group/18/"
            }
        },
        {
            "flavor_text": "\u81ea\u5206\u304c\u3000\u306a\u3063\u3066\u3057\u307e\u3063\u305f\n\u3069\u304f\u3084\u3000\u307e\u3072\u3084\u3000\u3084\u3051\u3069\u3092\n\u76f8\u624b\u306b\u3000\u3046\u3064\u3059\u3002",
