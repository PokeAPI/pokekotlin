        {
            "effect_entries": [
                {
                    "effect": "Redirects electric moves without negating them or granting any Special Attack boost.  Does not redirect hidden power.",
                    "language": {
                        "name": "en",
                        "url": "/api/v2/language/9/"
                    }
                }
            ],
            "version_group": {
                "name": "black-white",
