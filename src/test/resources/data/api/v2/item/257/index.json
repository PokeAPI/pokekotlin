            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u7d30\u304f\u3066\u3000\u9577\u3044\u3000\u771f\u3063\u8d64\u306a\u3000\u7cf8\u3002\n\u6301\u305f\u305b\u308b\u3068\u3000\u30e1\u30ed\u30e1\u30ed\u306b\u3000\u306a\u3063\u305f\u3068\u304d\n\u76f8\u624b\u3082\u3000\u30e1\u30ed\u30e1\u30ed\u306b\u3000\u3067\u304d\u308b\u3002",
