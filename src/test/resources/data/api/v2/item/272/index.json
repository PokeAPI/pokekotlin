            }
        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u4e08\u592b\u3067\u3000\u786c\u3044\u3000\u306c\u3051\u304c\u3089\u3002\n\u6301\u305f\u305b\u305f\u3000\u30dd\u30b1\u30e2\u30f3\u306f\u3000\u63a7\u3048\u306e\n\u30dd\u30b1\u30e2\u30f3\u3068\u3000\u5fc5\u305a\u3000\u3044\u308c\u304b\u308f\u308c\u308b\u3002",
