        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u30d5\u30bf\u30d0\u30bf\u30a6\u30f3\u304b\u3089\u3000\u65c5\u7acb\u3063\u305f\n\u5e7c\u306a\u3058\u307f\u306b\u3000\u6e21\u3059\u3088\u3046\u306b\n\u983c\u307e\u308c\u305f\u3000\u304a\u3068\u3069\u3051\u3082\u306e\u3002",
