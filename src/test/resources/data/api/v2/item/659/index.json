        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u96fb\u6c17\u3092\u3000\u307e\u3068\u3063\u3066\n\u76f8\u624b\u306b\u3000\u3076\u3064\u304b\u3063\u3066\u3000\u653b\u6483\u3059\u308b\u3002\n\u81ea\u5206\u3082\u3000\u5c11\u3057\u3000\u30c0\u30e1\u30fc\u30b8\u3092\u3000\u53d7\u3051\u308b\u3002",
