        },
        {
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "text": "\u30ad\u30e3\u30d7\u30c6\u30f3\u3000\u30b9\u30a4\u30ec\u30f3\u304c\u3000\u4f5c\u3063\u305f\n\u3064\u308a\u3056\u304a\u3002\u3000\u5ca9\u9670\u306b\u3000\u5411\u304b\u3063\u3066\n\u4f7f\u3046\u3068\u3000\u30dd\u30b1\u30e2\u30f3\u304c\u3000\u3064\u308c\u308b\u3002",
