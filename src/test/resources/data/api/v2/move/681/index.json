    "flavor_text_entries": [
        {
            "flavor_text": "\u8000\u6b66\u626c\u5a01\u5730\u653b\u51fb\u5bf9\u624b\uff0c\n\u81ea\u5df1\u7684\u80fd\u529b\u63d0\u9ad8\u5f97\u8d8a\u591a\uff0c\n\u5a01\u529b\u5c31\u8d8a\u5927\u3002",
