    "flavor_text_entries": [
        {
            "flavor_text": "\u5148\u52a0\u70ed\u9e1f\u5634\u540e\u518d\u8fdb\u884c\u653b\u51fb\u3002\n\u9e1f\u5634\u5728\u52a0\u70ed\u65f6\u5bf9\u624b\u89e6\u78b0\u7684\u8bdd\uff0c\n\u5c31\u4f1a\u4f7f\u5176\u707c\u4f24\u3002",
