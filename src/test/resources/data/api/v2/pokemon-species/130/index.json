        },
        {
            "flavor_text": "There are people who swear that any place\nGyarados appears is fated for destruction.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "version": {
                "name": "moon",
                "url": "/api/v2/version/28/"
