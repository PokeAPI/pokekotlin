        {
            "flavor_text": "\u7dba\u9e97\u306a\u3000\u6c34\u8fba\u304c\u3000\u4e3b\u306a\u3000\u68f2\u307f\u51e6\u3002\n\u5916\u6575\u306b\u3000\u8972\u308f\u308c\u305d\u3046\u306b\u3000\u306a\u308b\u3068\n\u6c34\u306b\u3000\u98db\u3073\u8fbc\u307f\u3000\u59ff\u3092\u3000\u96a0\u3059\u3002",
