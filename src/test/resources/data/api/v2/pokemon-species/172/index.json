    "flavor_text_entries": [
        {
            "flavor_text": "\u4ecd\u65e7\u5f88\u4e0d\u64c5\u957f\u4f7f\u7528\u7535\u529b\u3002\n\u4e00\u65e6\u5403\u60ca\u6216\u662f\u60c5\u7eea\u9ad8\u6da8\uff0c\n\u5c31\u4f1a\u7acb\u523b\u5f00\u59cb\u653e\u7535\u3002",
