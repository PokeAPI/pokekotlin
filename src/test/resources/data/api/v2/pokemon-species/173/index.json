        {
            "flavor_text": "\u6d41\u308c\u661f\u306e\u3000\u591a\u3044\u3000\u591c\u306f\u3000\u8f2a\u306b\u3000\u306a\u3063\u3066\n\u8e0a\u308b\u3000\u30d4\u30a3\u306e\u3000\u59ff\u3092\u3000\u202f\u308b\u3053\u3068\u304c\u3000\u3067\u304d\u308b\u3002\n\u65e5\u306e\u51fa\u307e\u3067\u3000\u8e0a\u308a\u3000\u591c\u9732\u3067\u3000\u306e\u3069\u3092\u3000\u6f64\u3059\u3002",
