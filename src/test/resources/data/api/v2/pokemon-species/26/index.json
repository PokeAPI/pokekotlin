        {
            "flavor_text": "\u8eab\u4f53\u306b\u3000\u96fb\u6c17\u304c\u3000\u305f\u307e\u308b\u306b\u3000\u3064\u308c\n\u653b\u6483\u7684\u306b\u3002\u3000\u5b9f\u306f\u3000\u96fb\u6c17\u304c\n\u30b9\u30c8\u30ec\u30b9\u306a\u306e\u3067\u306f\u3000\u3068\u3044\u3046\u3000\u8aac\u3082\u3042\u308b\u3002",
