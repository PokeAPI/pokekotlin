        {
            "flavor_text": "\u5371\u967a\u3092\u3000\u611f\u3058\u308b\u3068\u3000\u4f53\u3092\u3000\u9707\u308f\u305b\u3066\n\u982d\u306e\u3000\u3066\u3063\u307a\u3093\u304b\u3089\u3000\u80de\u5b50\u3092\u3000\u3070\u3089\u307e\u304f\u305e\u3002\n\u8349\u6728\u3082\u3000\u3057\u304a\u308c\u3066\u3057\u307e\u3046\u307b\u3069\u306e\u3000\u731b\u6bd2\u3060\u3002",
            "language": {
                "name": "ja",
