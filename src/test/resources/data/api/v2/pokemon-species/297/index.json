            },
            "version": {
                "name": "moon",
                "url": "/api/v2/version/28/"
            }
        },
        {
            "flavor_text": "Con uno dei suoi potentissimi schiaffi pu\u00f2\nscaraventare lontano un camion.\nAdora le gare di forza.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
