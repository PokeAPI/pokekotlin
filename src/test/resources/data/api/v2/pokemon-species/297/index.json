        {
            "flavor_text": "\u529b\u6bd4\u3079\u304c\u3000\u597d\u304d\u3060\u304c\u3000\u512a\u3057\u3055\u3082\n\u3042\u308f\u305b\u3082\u3061\u3000\u6226\u3063\u305f\u3000\u76f8\u624b\u3092\n\u306d\u304e\u3089\u3046\u306a\u3069\u3000\u793c\u5100\u3092\u3000\u91cd\u3093\u3058\u308b\u3002",
