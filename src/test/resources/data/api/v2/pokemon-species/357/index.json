        {
            "flavor_text": "\u5357\u56fd\u306e\u3000\u5b50\u3069\u3082\u305f\u3061\u306f\u3000\u30c8\u30ed\u30d4\u30a6\u30b9\u306e\u3000\u9996\u306b\n\u306a\u3063\u305f\u3000\u679c\u7269\u306e\u3000\u30d5\u30b5\u3092\u3000\u304a\u3084\u3064\u306b\u3000\u98df\u3079\u308b\u3088\u3002\n\u80cc\u4e2d\u306e\u3000\u8449\u3063\u3071\u3092\u3000\u7fbd\u3070\u305f\u304b\u305b\u3066\u3000\u7a7a\u3092\u3000\u98db\u3076\u3002",
