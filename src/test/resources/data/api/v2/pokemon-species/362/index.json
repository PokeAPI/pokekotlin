            "version": {
                "name": "moon",
                "url": "/api/v2/version/28/"
            }
        },
        {
            "flavor_text": "Congela le prede in un istante soffiando\naria gelida dalla grande bocca e poi\nle sgranocchia ancora ghiacciate.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
