    "flavor_text_entries": [
        {
            "flavor_text": "\u56e0\u4e3a\u7ec8\u4e8e\u80fd\u98de\u7684\u559c\u60a6\uff0c\n\u800c\u5728\u5929\u7a7a\u56db\u5904\u75be\u98de\u3002\n\u4e0d\u5230\u7cbe\u75b2\u529b\u7aed\u8981\u7761\u89c9\u5c31\u51e0\u4e4e\u4e0d\u843d\u5730\u3002",
