        },
        {
            "flavor_text": "\u3086\u3081\u306b\u307e\u3067\u3000\u202f\u305f\u3000\u3064\u3070\u3055\u304c\u3000\u3084\u3063\u3068\u3000\u306f\u3048\u3066\u304d\u305f\u3002\n\u3046\u308c\u3057\u3044\u3000\u304d\u3082\u3061\u3092\u3000\u3042\u3089\u308f\u3059\u305f\u3081\u3000\u304a\u304a\u305e\u3089\u3092\n\u3068\u3073\u307e\u308f\u308a\u3000\u307b\u306e\u304a\u3092\u3000\u306f\u3044\u3066\u3000\u3088\u308d\u3053\u3093\u3067\u3044\u308b\u3002",
