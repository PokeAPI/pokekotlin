        },
        {
            "flavor_text": "\u7ffc\u304c\u3000\u6b32\u3057\u3044\u3068\u3000\u5f37\u304f\u3000\u601d\u3044\u3000\u7d9a\u3051\u3066\u304d\u305f\n\u7d50\u679c\u3000\u4f53\u306e\u3000\u7d30\u80de\u304c\u3000\u7a81\u7136\u5909\u7570\u3092\n\u8d77\u3053\u3057\u3000\u898b\u4e8b\u306a\u3000\u7ffc\u304c\u3000\u751f\u3048\u3066\u304d\u305f\u3068\u3000\u3044\u3046\u3002",
