    "flavor_text_entries": [
        {
            "flavor_text": "\u611f\u60c5\u3092\u3000\u30e1\u30ed\u30c7\u30a3\u3067\u3000\u8868\u3059\u3002\n\u30e1\u30ed\u30c7\u30a3\u306e\u3000\u6cd5\u5247\u6027\u3092\n\u5b66\u8005\u305f\u3061\u304c\u3000\u7814\u7a76\u3057\u3066\u3044\u308b\u3002",
