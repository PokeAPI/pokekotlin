    "flavor_text_entries": [
        {
            "flavor_text": "\u5371\u967a\u3092\u3000\u611f\u3058\u308b\u3068\u3000\u5168\u8eab\u306e\n\u4f53\u6bdb\u304c\u3000\u5149\u308b\u3002\u76f8\u624b\u304c\u3000\u76ee\u3092\n\u304f\u3089\u307e\u305b\u3066\u3044\u308b\u3000\u3042\u3044\u3060\u306b\u3000\u9003\u3052\u308b\u3002",
