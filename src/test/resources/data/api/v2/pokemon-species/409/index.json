    "flavor_text_entries": [
        {
            "flavor_text": "\u8fc7\u53bb\u7684\u4eba\u4eec\u66fe\u5728\u6316\u51fa\u5316\u77f3\u540e\uff0c\n\u7528\u6bd4\u94a2\u94c1\u8fd8\u786c\u7684\u5934\u76d6\u9aa8\u505a\u6210\u5934\u76d4\u3002",
