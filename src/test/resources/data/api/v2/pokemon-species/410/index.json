            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\uc628\uc21c\ud55c \ucd08\uc2dd \ud3ec\ucf13\ubaac.\n\ud2bc\ud2bc\ud55c \uc5bc\uad74 \ud53c\ubd80\ub85c \uc9c0\uba74\uc744\n\ud30c\uace0\ub4e4\uc5b4 \ub098\ubb34 \ubfcc\ub9ac\ub3c4 \uba39\uc5c8\ub2e4.",
