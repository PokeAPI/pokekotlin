    "flavor_text_entries": [
        {
            "flavor_text": "\u56f4\u7ed5\u7740\u8212\u9002\u7684\u6811\u6728\u7684\u95ee\u9898\uff0c\n\u4e00\u76f4\u5728\u548c\u6295\u63b7\u7334\u7fa4\u4f53\u4e89\u593a\u9886\u5730\u3002\n\u4f46\u7ed3\u679c\u662f\u4e94\u4e94\u5206\u3002",
