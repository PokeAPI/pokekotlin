        {
            "flavor_text": "\u50b3\u8aaa\u5982\u679c\u88ab\u4e58\u8457\u98a8\n\u5728\u66ae\u8272\u88e1\u98db\u884c\u7684\u96a8\u98a8\u7403\u6349\u4f4f\uff0c\n\u5c31\u6703\u88ab\u5e36\u5230\u9670\u9593\u53bb\u3002",
