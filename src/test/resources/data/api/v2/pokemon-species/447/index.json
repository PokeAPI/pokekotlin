    "flavor_text_entries": [
        {
            "flavor_text": "\u80fd\u770b\u89c1\u88ab\u79f0\u4e3a\u6ce2\u5bfc\u7684\u4e00\u79cd\u6ce2\uff0c\n\u611f\u77e5\u4eba\u548c\u5b9d\u53ef\u68a6\u7684\u5fc3\u60c5\u3002\n\u4e0d\u4f1a\u9760\u8fd1\u5371\u9669\u7684\u5bf9\u624b\u3002",
