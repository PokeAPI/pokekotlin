    "flavor_text_entries": [
        {
            "flavor_text": "\u975e\u5e38\u4e0d\u8ba4\u751f\uff0c\u5f88\u5bb9\u6613\u4eb2\u8fd1\u3002\n\u4e0e\u5c0f\u7403\u98de\u9c7c\u7684\u4e92\u52a8\u5de1\u6e38\n\u662f\u975e\u5e38\u53d7\u6b22\u8fce\u7684\u6d77\u6ee9\u6d3b\u52a8\u3002",
