        },
        {
            "flavor_text": "\u308f\u304b\u3044\u3000\u30ea\u30fc\u30d5\u30a3\u30a2\u307b\u3069\u3000\u30c4\u30f3\u3068\u3000\u304f\u308b\n\u3042\u304a\u304f\u3055\u3044\u3000\u306b\u304a\u3044\u3002\u3000\u3068\u3057\u304a\u3044\u308b\u3068\n\u304a\u3061\u3070\u306e\u3000\u3088\u3046\u306a\u3000\u306b\u304a\u3044\u306b\u3000\u306a\u308b\u3002",
