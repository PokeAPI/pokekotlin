    "flavor_text_entries": [
        {
            "flavor_text": "\u6c17\u914d\u3092\u3000\u6bba\u3057\u3066\u3000\u63a5\u8fd1\u3059\u308b\u3002\n\u76f8\u624b\u304c\u3000\u6c17\u3065\u304f\u3000\u524d\u306b\u3000\u80cc\u5f8c\u304b\u3089\n\u5fcd\u3073\u5bc4\u308a\u3000\u3057\u3068\u3081\u3066\u3057\u307e\u3046\u306e\u3060\u3002",
