    "flavor_text_entries": [
        {
            "flavor_text": "\u6c34\u4e2d\u3068\u3000\u9678\u5730\u3067\u3000\u66ae\u3089\u3059\u3002\n\u9577\u304f\u3000\u306d\u3070\u306d\u3070\u3059\u308b\u3000\u30d9\u30ed\u3092\u4f7f\u3063\u3066\n\u76f8\u624b\u306e\u3000\u52d5\u304d\u3092\u3000\u5c01\u3058\u3053\u3081\u308b\u3002",
