    "flavor_text_entries": [
        {
            "flavor_text": "\u81ea\u5206\u3088\u308a\u3000\u5927\u304d\u306a\u3000\u76f8\u624b\u3068\n\u51fa\u4f1a\u3046\u3068\u3000\u7121\u6027\u306b\u3000\u6295\u3052\u305f\u304f\u306a\u308b\u3002\n\u5f37\u304f\u306a\u308b\u3068\u3000\u5e2f\u3092\u3000\u53d6\u308a\u66ff\u3048\u308b\u3002",
