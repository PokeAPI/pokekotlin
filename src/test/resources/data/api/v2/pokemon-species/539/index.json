    "flavor_text_entries": [
        {
            "flavor_text": "\u5e2f\u3092\u3000\u7de0\u3081\u308b\u3068\u3000\u6c17\u5408\u3044\u304c\u3000\u5165\u308a\n\u30d1\u30f3\u30c1\u306e\u3000\u7834\u58ca\u529b\u304c\u3000\u5897\u3059\u3002\n\u4fee\u884c\u3092\u3000\u30b8\u30e3\u30de\u3059\u308b\u3068\u3000\u6012\u308b\u3002",
