        {
            "flavor_text": "\u3075\u3057\u304e\u306a\u3000\u3061\u304b\u3089\u3092\u3000\u3064\u304b\u3063\u305f\u3000\u304d\u304a\u304f\u304c\u3000\u306a\u3044\u306e\u306f\n\u3061\u304b\u3089\u3092\u3000\u306f\u3063\u304d\u3057\u305f\u3000\u3068\u304d\u306f\u3000\u3058\u3085\u304f\u3059\u3044\u3057\u3066\u3044\u308b\n\u3068\u304d\u3068\u3000\u304a\u306a\u3058\u3000\u3058\u3087\u3046\u305f\u3044\u3060\u304b\u3089\u3000\u3089\u3057\u3044\u3002",
