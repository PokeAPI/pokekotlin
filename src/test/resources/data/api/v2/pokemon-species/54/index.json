        {
            "flavor_text": "\u5ff5\u529b\u3092\u3000\u4f7f\u3046\u3068\u3000\u982d\u75db\u304c\n\u3059\u308b\u306e\u3067\u3000\u666e\u6bb5\u306f\u3000\u306a\u308b\u3079\u304f\u3000\u4f55\u3082\n\u3057\u306a\u3044\u3067\u3000\u30dc\u30fc\u3063\u3068\u3000\u904e\u3054\u3057\u3066\u3044\u308b\u3088\u3002",
