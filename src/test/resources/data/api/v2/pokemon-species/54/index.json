        {
            "flavor_text": "\u4e0d\u601d\u8b70\u306a\u3000\u529b\u3092\u3000\u4f7f\u3063\u305f\u3000\u8a18\u61b6\u304c\u3000\u306a\u3044\u306e\u306f\n\u529b\u3092\u3000\u767a\u63ee\u3057\u305f\u3000\u3068\u304d\u306f\u3000\u719f\u7761\u3057\u3066\u3044\u308b\n\u3068\u304d\u3068\u3000\u540c\u3058\u3000\u72b6\u614b\u3060\u304b\u3089\u3000\u3089\u3057\u3044\u3002",
