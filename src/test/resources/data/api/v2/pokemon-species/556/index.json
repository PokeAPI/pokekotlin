    "flavor_text_entries": [
        {
            "flavor_text": "\u4e7e\u71e5\u3057\u305f\u3000\u571f\u5730\u3067\u3000\u66ae\u3089\u3059\u3002\n\u4f53\u3092\u3000\u30ea\u30ba\u30df\u30ab\u30eb\u306b\u3000\u52d5\u304b\u3059\u3068\n\u30de\u30e9\u30ab\u30b9\u306e\u3088\u3046\u306a\u3000\u97f3\u3092\u3000\u594f\u3067\u308b\u3002",
