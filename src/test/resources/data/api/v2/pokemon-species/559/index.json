    "flavor_text_entries": [
        {
            "flavor_text": "\u51e0\u53ea\u4e3a\u4e00\u7ec4\u884c\u52a8\u3002\n\u6f2b\u65e0\u76ee\u7684\u5730\u5230\u5904\u8d70\uff0c\u4f1a\u51b7\u4e0d\u4e01\u5730\n\u5411\u89c6\u7ebf\u76f8\u9047\u7684\u5bf9\u624b\u65bd\u4ee5\u5934\u9524\u3002",
