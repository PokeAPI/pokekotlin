            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\uac70\uce60\uace0 \ub09c\ud3ed\ud558\uc9c0\ub9cc, \uc790\uc2e0\uc758 \uac00\uc871\uacfc\n\ubb34\ub9ac \ub3d9\ub8cc\uc758 \uc601\uc5ed\uc744 \ub9e4\uc6b0\n\uc18c\uc911\ud788 \uc5ec\uae30\ub294 \ud3ec\ucf13\ubaac\uc774\ub2e4.",
