        },
        {
            "flavor_text": "\u30c8\u30b5\u30ab\u306e\u3000\u5927\u304d\u3055\u3067\u3000\u30b0\u30eb\u30fc\u30d7\u306e\n\u30ea\u30fc\u30c0\u30fc\u3092\u3000\u6c7a\u3081\u308b\u3002\u30ad\u30c3\u30af\u653b\u6483\u3067\n\u30b3\u30f3\u30af\u30ea\u30fc\u30c8\u30d6\u30ed\u30c3\u30af\u3092\u3000\u7834\u58ca\u3059\u308b\u3002",
