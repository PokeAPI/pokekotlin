    "flavor_text_entries": [
        {
            "flavor_text": "\u6301\u3063\u3066\u3044\u308b\u3000\u30de\u30b9\u30af\u306f\n\u30c7\u30b9\u30de\u30b9\u304c\u3000\u4eba\u9593\u3060\u3063\u305f\u3000\u3068\u304d\u306e\u3000\u9854\u3002\n\u305f\u307e\u306b\u3000\u898b\u3064\u3081\u3066\u306f\u3000\u6ce3\u3044\u3066\u3044\u308b\u3002",
