        {
            "flavor_text": "\u304a\u5893\u306b\u3000\u57cb\u846c\u3055\u308c\u305f\u3000\u4eba\u306e\n\u9b42\u304c\u3000\u30dd\u30b1\u30e2\u30f3\u306b\u3000\u5909\u5316\u3057\u305f\u3002\n\u6b7b\u306c\u524d\u306e\u3000\u8a18\u61b6\u304c\u3000\u6b8b\u3063\u3066\u3044\u308b\u3002",
