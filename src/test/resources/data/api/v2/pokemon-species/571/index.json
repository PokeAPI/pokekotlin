        },
        {
            "flavor_text": "\u4f55\u304b\u306b\u3000\u5316\u3051\u308b\u3060\u3051\u3000\u3067\u306f\u306a\u304f\n\u4f55\u767e\u4eba\u3082\u306e\u3000\u4eba\u306b\n\u5e7b\u3092\u3000\u898b\u305b\u308b\u3000\u529b\u3092\u3082\u3064\u3002",
