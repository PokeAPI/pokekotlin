            "version": {
                "name": "omega-ruby",
                "url": "/api/v2/version/25/"
            }
        },
        {
            "flavor_text": "Eating a twig fills it with energy,\nand its roomy ears give vent to air\nhotter than 390 degrees Fahrenheit.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
