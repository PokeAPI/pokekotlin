    "flavor_text_entries": [
        {
            "flavor_text": "\u6728\u306e\u679d\u3092\u3000\u5c3b\u5c3e\u304b\u3089\u3000\u5f15\u304d\u629c\u304f\u3068\u304d\n\u6469\u64e6\u3067\u3000\u7740\u706b\u3002\u679d\u306e\u3000\u708e\u3092\u3000\u632f\u3063\u3066\n\u4ef2\u9593\u306b\u3000\u5408\u56f3\u3092\u3000\u9001\u308b\u3002",
