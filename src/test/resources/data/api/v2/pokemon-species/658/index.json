    "flavor_text_entries": [
        {
            "flavor_text": "\u5fcd\u8005\u306e\u3088\u3046\u306b\u3000\u795e\u51fa\u9b3c\u6ca1\u3002\n\u7d20\u65e9\u3044\u3000\u52d5\u304d\u3067\u3000\u7ffb\u5f04\u3057\u3064\u3064\n\u6c34\u306e\u3000\u624b\u88cf\u5263\u3067\u3000\u5207\u308a\u88c2\u304f\u3002",
