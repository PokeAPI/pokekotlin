            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
            }
        },
        {
            "flavor_text": "\u6c34\u3092\u3000\u5727\u7e2e\u3057\u3066\u3000\u624b\u88cf\u5263\u3092\u3000\u4f5c\u308a\u51fa\u3059\u3002\n\u9ad8\u901f\u56de\u8ee2\u3055\u305b\u3066\u3000\u98db\u3070\u3059\u3068\n\u91d1\u5c5e\u3082\u3000\u771f\u3063\u4e8c\u3064\u3002",
