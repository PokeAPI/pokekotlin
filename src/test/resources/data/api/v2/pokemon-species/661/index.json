        {
            "flavor_text": "\u3055\u3048\u305a\u308b\u3000\u58f0\u306f\u3000\u7f8e\u3057\u3044\u304c\n\u7e04\u5f35\u308a\u306b\u3000\u5165\u3063\u305f\u3000\u76f8\u624b\u306f\n\u5bb9\u8d66\u3057\u306a\u3044\u3000\u8352\u3005\u3057\u3055\u3060\u3002",
