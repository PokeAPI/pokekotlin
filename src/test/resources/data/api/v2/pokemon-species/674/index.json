        },
        {
            "flavor_text": "\u54a5\u3048\u3066\u3044\u308b\u3000\u8449\u3063\u3071\u306b\u3000\u610f\u5473\u306f\u3000\u306a\u304f\n\u305f\u3060\u306e\u3000\u304b\u3063\u3053\u3064\u3051\u3002\u3000\u3084\u3093\u3061\u3083\u306a\u306e\u3067\n\u7d20\u4eba\u30c8\u30ec\u30fc\u30ca\u30fc\u306b\u306f\u3000\u5411\u304b\u306a\u3044\u3002",
