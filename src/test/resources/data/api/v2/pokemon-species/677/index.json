    "flavor_text_entries": [
        {
            "flavor_text": "\uff11\uff10\uff10\u30e1\u30fc\u30c8\u30eb\u4ee5\u5185\u306e\u3000\u3082\u306e\u3092\n\u5439\u304d\u98db\u3070\u3059\u3000\u307b\u3069\u306e\u3000\u30b5\u30a4\u30b3\u30d1\u30ef\u30fc\u3092\n\u3046\u307e\u304f\u3000\u30b3\u30f3\u30c8\u30ed\u30fc\u30eb\u3000\u3067\u304d\u306a\u3044\u306e\u3060\u3002",
