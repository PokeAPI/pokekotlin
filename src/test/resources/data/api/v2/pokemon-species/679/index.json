    "flavor_text_entries": [
        {
            "flavor_text": "\u5263\u306e\u3000\u67c4\u3092\u3000\u63e1\u3063\u305f\u3000\u4eba\u306e\u3000\u8155\u306b\n\u9752\u3044\u3000\u5e03\u3092\u3000\u5dfb\u304d\u3064\u3051\u3066\n\u5012\u308c\u308b\u307e\u3067\u3000\u547d\u3092\u3000\u5438\u3044\u53d6\u308b\u3002",
