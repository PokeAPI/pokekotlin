    "flavor_text_entries": [
        {
            "flavor_text": "\uff12\u672c\u306e\u3000\u5263\u306b\u3088\u308b\n\u8907\u96d1\u306a\u3000\u9023\u7d9a\u653b\u6483\u3092\u3000\u9632\u3050\u3053\u3068\u306f\n\u5263\u306e\u3000\u9054\u4eba\u3067\u3082\u3000\u4e0d\u53ef\u80fd\u3060\u3002",
