    "flavor_text_entries": [
        {
            "flavor_text": "\u306b\u304a\u3044\u304c\u3000\u5f37\u3059\u304e\u308b\u305f\u3081\n\u3088\u307b\u3069\u3000\u597d\u304d\u306a\u3000\u30c8\u30ec\u30fc\u30ca\u30fc\u3067\u306a\u3051\u308c\u3070\n\u9023\u308c\u3066\u3000\u6b69\u304f\u306e\u306f\u3000\u30ad\u30c4\u30a4\u306e\u3060\u3002",
