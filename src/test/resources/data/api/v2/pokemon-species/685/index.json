        },
        {
            "flavor_text": "\u304d\u3085\u3046\u304b\u304f\u306f\u3000\u3072\u3068\u306e\u3000\uff11\u304a\u304f\u3070\u3044\u3002\n\u304f\u3046\u304d\u3061\u3085\u3046\u306e\u3000\u308f\u305a\u304b\u306a\u3000\u306b\u304a\u3044\u3067\n\u307e\u308f\u308a\u306e\u3000\u3088\u3046\u3059\u304c\u3000\u3059\u3079\u3066\u3000\u308f\u304b\u308b\u3002",
