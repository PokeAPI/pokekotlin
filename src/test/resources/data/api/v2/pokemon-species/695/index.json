    "flavor_text_entries": [
        {
            "flavor_text": "\u96fb\u6c17\u3067\u3000\u7b4b\u8089\u3092\u3000\u523a\u6fc0\u3059\u308b\u3068\n\uff11\uff10\uff10\u30e1\u30fc\u30c8\u30eb\u3092\u3000\uff15\u79d2\u3067\u3000\u8d70\u308b\n\u811a\u529b\u306b\u3000\u30d1\u30ef\u30fc\u30a2\u30c3\u30d7\u3059\u308b\u3002",
