    "flavor_text_entries": [
        {
            "flavor_text": "\u80cc\u4e2d\u306e\u3000\u30a2\u30fc\u30e0\u304b\u3089\n\u4f53\u5185\u306e\u3000\u6c34\u84b8\u6c17\u3092\u3000\u5674\u5c04\u3059\u308b\u3002\n\u5c71\u3000\u3072\u3068\u3064\u3000\u5439\u304d\u98db\u3070\u3059\u3000\u5a01\u529b\u3002",
