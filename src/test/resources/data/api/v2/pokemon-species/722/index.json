            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u306f\u3082\u306e\u306e\u3000\u3088\u3046\u306b\u3000\u3059\u308b\u3069\u3044\u3000\u306f\u306d\u3092\n\u3068\u3070\u3057\u3066\u3000\u3053\u3046\u3052\u304d\u3002\u3000\u3042\u3057\u306e\u3061\u304b\u3089\u3082\n\u3064\u3088\u304f\u3000\u30ad\u30c3\u30af\u3082\u3000\u3042\u306a\u3069\u308c\u306a\u3044\u306e\u3060\u3002",
