    "flavor_text_entries": [
        {
            "flavor_text": "\u975e\u5e38\u81ea\u604b\uff0c\u53c8\u7231\u7f8e\u3002\n\u5982\u679c\u4e0d\u52e4\u5feb\u5730\u7ed9\u5b83\u6253\u7406\uff0c\n\u6216\u8bb8\u4f1a\u4e0d\u542c\u8bdd\u3002",
