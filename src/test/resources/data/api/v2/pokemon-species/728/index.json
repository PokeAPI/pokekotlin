    "flavor_text_entries": [
        {
            "flavor_text": "\u901a\u8fc7\u6bcf\u5929\u4e0d\u65ad\u5730\u91cd\u590d\u7ec3\u4e60\uff0c\n\u7528\u9f3b\u5b50\u5439\u8d77\u6765\u7684\u6c14\u7403\n\u4e00\u70b9\u70b9\u5730\u53d8\u5927\u8d77\u6765\u3002",
