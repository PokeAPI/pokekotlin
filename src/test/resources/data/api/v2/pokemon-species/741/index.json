        },
        {
            "flavor_text": "\u60c5\u71b1\u7684\u306a\u3000\u30c0\u30f3\u30b9\u3067\u3000\u6575\u306e\n\u5fc3\u3092\u3000\u3064\u304b\u307f\u3000\u6cb9\u65ad\u3057\u305f\u3000\u9699\u306b\n\u71c3\u3048\u3055\u304b\u308b\u3000\u708e\u3067\u3000\u713c\u304d\u5c3d\u304f\u3059\u3002",
