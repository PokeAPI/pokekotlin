    "flavor_text_entries": [
        {
            "flavor_text": "\u7528\u70ed\u60c5\u7684\u821e\u8e48\u6293\u4f4f\u654c\u4eba\u7684\u5fc3\uff0c\n\u8d81\u5bf9\u65b9\u758f\u5ffd\u7684\u95f4\u9699\uff0c\n\u7528\u718a\u718a\u71c3\u70e7\u7684\u706b\u7130\u5c06\u5176\u70e7\u5c3d\u3002",
