        },
        {
            "flavor_text": "\u3079\u3064\u305b\u304b\u3044\u306b\u3000\u3059\u3080\u3000\u305b\u3044\u3081\u3044\u305f\u3044\u3002\n\u307b\u305d\u304f\u3000\u3057\u306a\u3084\u304b\u306a\u3000\u304b\u3089\u3060\u3000\u3060\u304c\n\u3064\u3088\u3044\u30d1\u30ef\u30fc\u3092\u3000\u3072\u3081\u3066\u3044\u308b\u3000\u3088\u3046\u3059\u3002",
