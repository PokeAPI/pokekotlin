        {
            "flavor_text": "\u30ab\u30e9\u306e\u3000\u4e2d\u306b\u3000\u305f\u307e\u3063\u305f\u3000\u7802\u306f\n\u3084\u304c\u3066\u3000\u771f\u73e0\u306b\u3000\u5909\u308f\u308b\u304c\n\u90aa\u9b54\u3000\u306a\u306e\u3067\u3000\u5410\u304d\u51fa\u3057\u3066\u3000\u6368\u3066\u308b\u3002",
