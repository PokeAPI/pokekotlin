    "flavor_text_entries": [
        {
            "flavor_text": "\u5728\u6ca1\u6709\u6708\u4eae\u7684\u665a\u4e0a\uff0c\n\u9b3c\u65af\u901a\u4f1a\u5bfb\u627e\u53ef\u4ee5\u8bc5\u5492\u7684\u5bf9\u8c61\uff0c\n\u6240\u4ee5\u4e0d\u8981\u5916\u51fa\u8d70\u52a8\u6bd4\u8f83\u597d\u54e6\u3002",
            "language": {
