                "url": "/api/v2/language/3/"
            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u308f\u308b\u3044\u3053\u306e\u3000\u3068\u3053\u308d\u306b\u306f\u3000\u30b2\u30f3\u30ac\u30fc\u304c\n\u3084\u3063\u3066\u304f\u308b\u3000\u3068\u3044\u3046\u3000\u3044\u3044\u3064\u305f\u3048\u306f\n\u305b\u304b\u3044\u3058\u3085\u3046\u3067\u3000\u304d\u304f\u3053\u3068\u304c\u3000\u3067\u304d\u308b\u3002",
