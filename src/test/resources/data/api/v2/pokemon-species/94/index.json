    "flavor_text_entries": [
        {
            "flavor_text": "\u5728\u4e16\u754c\u5404\u5730\u90fd\u80fd\u542c\u5230\n\u803f\u9b3c\u4f1a\u53bb\u627e\u574f\u5b69\u5b50\u8fd9\u6837\u7684\u4f20\u8bf4\u3002",
