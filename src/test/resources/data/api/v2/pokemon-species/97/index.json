        {
            "flavor_text": "\u624b\u306b\u3000\u6301\u3063\u3066\u3044\u308b\u3000\u632f\u308a\u5b50\u3000\u306e\u3000\u52d5\u304d\u3068\u3000\u8f1d\u304d\u304c\n\u76f8\u624b\u3092\u3000\u6df1\u3044\u3000\u50ac\u7720\u72b6\u614b\u306b\u3000\u843d\u3068\u3059\u3002\n\u7372\u7269\u3092\u3000\u63a2\u3057\u306a\u304c\u3089\u3000\u632f\u308a\u5b50\u3092\u3000\u78e8\u3044\u3066\u3044\u308b\u3002",
