    "flavor_text_entries": [
        {
            "flavor_text": "\u751f\u6d3b\u5728\u963f\u7f57\u62c9\u7684\u5f15\u68a6\u8c98\u4eba\u7684\n\u76ee\u6807\u4e3b\u8981\u662f\u6811\u6795\u5c3e\u718a\u3002\n\u4eba\u5012\u4e0d\u600e\u4e48\u4f1a\u6210\u4e3a\u53d7\u5bb3\u8005\u3002",
